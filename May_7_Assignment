import java.util.*;
class demo
{
	public static void main(String args[])
	{	
		int num1,num2,res;
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter first number");
		num1=sc.nextInt();
		System.out.println("Enter second number");
		num2=sc.nextInt();
		res=num1+num2;
		System.out.println("The addition  is ="+res);
	}
}
